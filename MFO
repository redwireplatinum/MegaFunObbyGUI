-- Scripts:
function SCRIPT_EMAK78_FAKESCRIPT() -- Currentstage.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = Currentstage
    local stage = game.Players.LocalPlayer.leaderstats.Stage
	script.Parent.Text = stage.Value
    stage.Changed:connect(function()
    script.Parent.Text = stage.Value
    end)

end
coroutine.resume(coroutine.create(SCRIPT_EMAK78_FAKESCRIPT))
function SCRIPT_EHCG72_FAKESCRIPT() -- Endstage.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = Endstage
	local mps = game:GetService("MarketplaceService")
	
	local GetName, gamename = pcall(mps.GetProductInfo, mps, game.PlaceId)
	if GetName then
		namestring = gamename.Name
		namestring = namestring:gsub("Mega Fun Obby 2", "oof")
		maxstagenum = string.match(
			namestring,
		"%d+")
		script.Parent.Text = maxstagenum
	end

end
coroutine.resume(coroutine.create(SCRIPT_EHCG72_FAKESCRIPT))
function SCRIPT_PFMH88_FAKESCRIPT() -- stageskip.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = stageskip
	script.Parent.MouseButton1Down:connect(function()
		local start = script.Parent.Parent.Currentstage
		local endstage = script.Parent.Parent.Endstage
		for i = tonumber(start.Text), tonumber(endstage.Text) do
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.stages[i].CFrame
			wait(0.2)
		end
	end)

end
coroutine.resume(coroutine.create(SCRIPT_PFMH88_FAKESCRIPT))
function SCRIPT_LBRZ77_FAKESCRIPT() -- MainFrame.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = MainFrame
	MainFrame.Draggable = true

end
coroutine.resume(coroutine.create(SCRIPT_LBRZ77_FAKESCRIPT))
function SCRIPT_XLWT90_FAKESCRIPT() -- delkill.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = delkill
	script.Parent.MouseButton1Down:connect(function()
	for i, kill in ipairs(workspace.kill:GetChildren()) do
	if kill:IsA("BasePart") then
		if kill.Transparency ~= 1 then
			kill:Destroy()
		end
	end
end
	end)

end
coroutine.resume(coroutine.create(SCRIPT_XLWT90_FAKESCRIPT))
function SCRIPT_PFQI83_FAKESCRIPT() -- respawn.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = respawn
	script.Parent.MouseButton1Down:connect(function()
		game.Players.LocalPlayer.Character.Humanoid.Health = 0
	end)

end
coroutine.resume(coroutine.create(SCRIPT_PFQI83_FAKESCRIPT))
local lplr = game.Players.LocalPlayer
local plrgui = lplr.PlayerGui
local hrp = lplr.Character.HumanoidRootPart
if game.PlaceId == 12996397 then
plrgui.Chat.Frame.Changed:connect(function()
if plrgui.Chat.Frame.Visible ~= true then
game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, true)
end
end)
local skip = plrgui.MFO_UI.Groundbar.SKIPS
skip.RemainT.Text = "SKIPS LEFT : inf"
plrgui.Note.Contents.Info_Skip:Destroy()
skip.SELECT.MouseButton1Down:connect(function()
hrp.CFrame = workspace.stages[lplr.leaderstats.Stage.Value + 1].CFrame
end)
elseif game.PlaceId == 982130670 then
local f = plrgui.HUD.skipsLabel
local skips = f:Clone()
skips.Parent = f.Parent
f:Destroy()
skips.Text = "Skips Left: inf"
skips.MouseButton1Down:connect(function()
hrp.CFrame = workspace.stages[lplr.leaderstats.Stage.Value + 1].CFrame
end)
end
